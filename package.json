    "scripts": {
        "start": "export REACT_APP_IS_DEV_SERVER=true && yarn generate:api && yarn react-scripts start",
        "start:ios": "REACT_APP_IS_DEV_SERVER=true ionic capacitor run ios -l --external",
        "start:android": "REACT_APP_IS_DEV_SERVER=true ionic capacitor run android -l --external",
        "start:electron": "ionic capacitor open electron",
        "prepare:ios": "ionic capacitor copy ios",
        "prepare:android": "ionic capacitor copy android",
        "prepare:electron": "ionic cap copy electron",
        "serve:electron:build": "ELECTRON_IS_DEV=0 ionic capacitor open electron",
        "build": "yarn generate:api && yarn react-scripts build",
        "test": "react-scripts test --transformIgnorePatterns 'node_modules/(?!(@ionic/react|@ionic/react-router|@ionic/core|@stencil/core|ionicons)/)'",
        "eject": "react-scripts eject",
        "generate:api": "graphql-codegen --config codegen.yml"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
